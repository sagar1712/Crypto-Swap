generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url          = env("DATABASE_URL")
}

model Exchange {
  id                String @id @default(uuid())
  initialCurrency   String 
  exchangedCurrency String
  tradeAmount       Int

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}